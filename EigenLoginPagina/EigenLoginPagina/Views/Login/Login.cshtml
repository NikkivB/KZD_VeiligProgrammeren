


@using(Html.BeginForm("Login", "Login", FormMethod.Post))
            {
    @Html.AntiForgeryToken()
    if(ViewBag.Error != null)
    { 
    <p class="alert-danger">@ViewBag.Error</p>
    }
<table>
    <tr>
        <td>Username</td>
        <td><input type="text" name=" username"/></td>
    </tr>
    <tr>
        <td>Password</td>
        <td><input type="text" name="password"/></td>
    </tr>
</table>
<input type="submit" />
            }